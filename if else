#include<stdio.h>
int Sibi(int a,int b,int c,int d,int e,int f)
{
    int answer;
    if(a>b && a>c && a>d && a>e && a>f)
{
    answer=a;
}    

else if(b>a && b>c && b>d && b>e && b>f)
{
    answer=b;
}
else if(c>a  && c>b && c>d && c>e && c>f)
{
    answer=c;
}
else if(d>a && d>b && d>c && d>e && d>f)
{
    answer=d;
}
else if(e>a && e>b && e>c && e>d && e>f)
{
    answer=e;
}
else
{
  answer=f;  
}
return answer;
}

int main()
{
    printf("the answer is:%d\n",Sibi(1,30,45,28,50,34));
    return 0;
}
