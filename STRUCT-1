#include<stdio.h>
struct students
{
    char name[25];
    int g;
    char k;
    char city[30];
    char major[20];

};
int main()
{
    struct students s1={"sibi",76,'A',"trichy","ece"};
    struct students s2={"preethi",87,'B',"salem","eee"};
    struct students s3={"preesha",66,'A',"trichy","ece"};
    struct students s4={"sharmi",75,'A',"trichy","ece"};
    struct students s5={"swetha",93,'O',"trichy","ece"};
    printf("student 1:%s\t%d\t%c\t%s\t%s\n",s1.name,s1.g,s1.k,s1.city,s1.major);
    printf("student 2:%s\t%d\t%c\t%s\t%s\n",s2.name,s2.g,s2.k,s2.city,s2.major);
    printf("student 3:%s\t%d\t%c\t%s\t%s\n",s3.name,s3.g,s3.k,s3.city,s3.major);
    printf("student 4:%s\t%d\t%c\t%s\t%s\n",s4.name,s4.g,s4.k,s4.city,s4.major);
    printf("student 5:%s\t%d\t%c\t%s\t%s\n",s5.name,s5.g,s5.k,s5.city,s5.major);
    return 0;
}
